<button {attr} class="btn {style}">{label}</button>
<button {attr} class="btn {style} {size} {variation} {align} dropdown-toggle dropdown-toggle-split" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
    <span class="sr-only">Toggle Dropdown</span>
</button>
<div class="dropdown-menu" aria-labelledby="{id}">
    {content}
</div>
  