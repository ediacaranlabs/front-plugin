<div {attr} class="pricing-box-alt">
  <div class="pricing-heading">
    <h3>{title}</h3>
  </div>
  <div class="pricing-terms">
    <h6>{terms}</h6>
  </div>
  <div class="pricing-content">
    <ul>
    	{content}
    </ul>
  </div>
  <div class="pricing-action">
    <a href="{action}" class="btn btn-medium btn-theme"><i class="icon-bolt"></i> {label}</a>
  </div>
</div>